using System;
public class BreakContinue
{
    public static void Main()
    {
        for (int i=10; i<=20; i+=2)
        {
            if (i==16) 
                continue;
            Console.WriteLine(i);
        }
         
        for (int i=10; i<=20; i++)
        {
            if (i%2 == 1) 
                continue;               
            if (i==16) 
                continue;
            Console.WriteLine(i);
        }
         
         
        for (int i=10; ; i+=2)
        {
            if (i==16) 
                continue;
            if (i>20) 
                break;
            Console.WriteLine(i);
            Console.ReadLine();
             
        }       
    }
 }
