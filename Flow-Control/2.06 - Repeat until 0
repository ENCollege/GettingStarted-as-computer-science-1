using System;
public class Exercise022
{
    public static void Main()
    {
        int number;
         
        Console.Write("Enter a number: ");
        number = Convert.ToInt32(Console.ReadLine());
         
        while (number != 0)
        {
            Console.WriteLine(number * 10);
             
            Console.Write("Enter a number: ");
            number = Convert.ToInt32(Console.ReadLine());
            Console.ReadLine();
        }
    }
}
